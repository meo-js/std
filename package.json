            "require": "./dist/json.cjs",
            "types": "./dist/json.d.ts"
        },
        "./macro": {
            "import": "./dist/macro.mjs",
            "require": "./dist/macro.cjs",
            "types": "./dist/macro.d.ts"
        },
        "./object": {
            "import": "./dist/object.mjs",
