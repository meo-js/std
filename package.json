    },
    "files": [
        "dist",
        "README*.md",
        "LICENSE"
    ],
    "lint-staged": {
        "src/**/*": [
            "eslint",
            "prettier --ignore-unknown --check"
        ]
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "commit-msg": "npx commitlint --edit $1"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "scripts": {
        "build": "npx unbuild",
        "dev": "npx unbuild --watch",
        "clean": "rimraf ./dist",
        "lint": "eslint",
        "prepare": "simple-git-hooks",
        "gen-exports": "npx tsx ./scripts/exports-generator.ts"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@meojs/cfgs": "^2.0.13",
        "@meojs/types": "^1.0.1",
        "@types/node": "^20",
        "cross-env": "^7.0.3",
        "eslint": "^9.22.0",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "simple-git-hooks": "^2.11.1"
    },
    "dependencies": {
        "chardet": "^2.1.0",
        "core-js": "^3.41.0",
        "string-ts": "^2.2.1",
        "type-fest": "^4.40.1"
    }
}
